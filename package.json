    "ts-node": "10.0.0",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.2.4",
    "webpack": "5.38.1",
    "webpack-cli": "4.7.0"
