{
  "name": "anti-redirect",
  "version": "2.3.1",
  "description": "GM脚本, 去除各搜索引擎/常用网站的重定向",
  "main": "./dist/anti-redirect.user.js",
  "scripts": {
    "test": "echo 'Test success!'",
    "build": "cross-env NODE_ENV=production webpack --display-error-details --progress --colors",
    "watch": "cross-env NODE_ENV=development webpack --display-error-details --progress --colors --watch",
    "coverage": "istanbul cover ./**/*.js",
    "contributor": "./node_modules/.bin/contributors-stat",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "generate": "all-contributors generate",
    "deploy": "npm run build && gh-pages -d ./dist",
    "format": "prettier --single-quote -l --write 'src/**/*.{js,css,ts,json}'"
  },
  "author": "Axetroy",
  "license": "ISC",
  "keywords": [
    "greasy",
    "script",
    "javascript",
    "redirect"
  ],
  "devDependencies": {
    "@types/node": "^8.0.0",
    "all-contributors-cli": "^4.4.0",
    "contributors-stat": "^1.1.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.5",
    "gh-pages": "^1.0.0",
    "istanbul": "^0.4.5",
    "moment": "^2.18.1",
    "prettier": "^1.7.0",
    "ts-loader": "^2.3.4",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.2",
    "webpack": "3.6.0",
    "webpack-bundle-analyzer": "^2.9.0"
  },
  "dependencies": {
    "@axetroy/event-emitter.js": "^1.2.4",
    "conventional-changelog-cli": "^1.3.3",
    "es6-promise": "^4.1.1",
    "gm-http": "^0.1.4",
    "in-view": "^0.6.1",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "reflect-metadata": "^0.1.10"
  }
}
